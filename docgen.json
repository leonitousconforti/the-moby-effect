{
    "$schema": "./node_modules/@effect/docgen/schema.json",
    "exclude": ["src/internal/**/*.ts"],
    "projectHomepage": "https://github.com/leonitousconforti/the-wireguard-effect",
    "examplesCompilerOptions": {
        "noEmit": true,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "Bundler",
        "module": "ES2022",
        "target": "ES2022",
        "lib": ["ES2022"],
        "baseUrl": "../../",
        "paths": {
            "the-wireguard-effect": ["./src/index.js"],
            "the-wireguard-effect/*": ["./src/*.js"]
        }
    }
}
