{
    "name": "the-moby-effect",
    "version": "0.0.0",
    "description": "Moby/Docker API client built using effect-ts",
    "keywords": [
        "moby",
        "docker",
        "podman",
        "effect-ts"
    ],
    "homepage": "https://github.com/leonitousconforti/the-moby-effect",
    "bugs": {
        "url": "https://github.com/leonitousconforti/the-moby-effect/issues",
        "email": "leo@leoconforti.us"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leonitousconforti/the-moby-effect.git"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/dts/index.d.ts",
    "directories": {
        "bin": "bin",
        "doc": "docs",
        "example": "examples",
        "test": "test"
    },
    "scripts": {
        "build": "pnpm clean && pnpm codegen && pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v2 && docgen",
        "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
        "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
        "build-esm": "tsc -b tsconfig.build.json",
        "build-reflection": "(cd reflection/ && go build)",
        "build-schemas": "(cd src && rm -rf ./generated && mkdir -p generated && ../reflection/reflection) && eslint ./src/generated/ --fix",
        "check": "tsc -b tsconfig.json",
        "circular": "node ./scripts/circular.mjs",
        "clean": "rimraf .tsbuildinfo docs build dist coverage",
        "codegen": "build-utils prepare-v2",
        "codemod": "node scripts/codemod.mjs",
        "coverage": "vitest --coverage",
        "lint": "eslint .",
        "schemagen": "pnpm build-reflection && pnpm build-schemas",
        "test": "vitest",
        "update-blobs": "tsx ./scripts/update-blobs.ts"
    },
    "dependencies": {
        "eftar": "0.0.29"
    },
    "devDependencies": {
        "@babel/cli": "7.27.0",
        "@babel/core": "7.26.10",
        "@babel/plugin-transform-export-namespace-from": "7.25.9",
        "@babel/plugin-transform-modules-commonjs": "7.26.3",
        "@effect/build-utils": "0.7.9",
        "@effect/cli": "0.59.4",
        "@effect/cluster": "0.29.7",
        "@effect/docgen": "0.5.2",
        "@effect/eslint-plugin": "0.3.0",
        "@effect/experimental": "0.44.4",
        "@effect/language-service": "0.4.0",
        "@effect/platform": "0.80.4",
        "@effect/platform-browser": "0.59.4",
        "@effect/platform-bun": "0.60.7",
        "@effect/platform-node": "0.76.7",
        "@effect/printer": "0.42.4",
        "@effect/printer-ansi": "0.42.4",
        "@effect/rpc": "0.55.5",
        "@effect/sql": "0.33.4",
        "@effect/typeclass": "0.33.4",
        "@effect/vitest": "0.20.4",
        "@eslint/compat": "1.2.8",
        "@eslint/eslintrc": "3.3.1",
        "@eslint/js": "9.23.0",
        "@types/node": "22.13.17",
        "@types/ssh2": "1.15.4",
        "@types/ws": "8.18.1",
        "@typescript-eslint/eslint-plugin": "8.29.0",
        "@typescript-eslint/parser": "8.29.0",
        "@typescript-eslint/utils": "8.29.0",
        "@vitest/coverage-v8": "3.1.1",
        "ast-types": "0.14.2",
        "babel-plugin-annotate-pure-calls": "0.5.0",
        "effect": "3.14.4",
        "eslint": "9.23.0",
        "eslint-config-prettier": "10.1.1",
        "eslint-import-resolver-typescript": "4.3.1",
        "eslint-plugin-import-x": "4.10.0",
        "eslint-plugin-prettier": "5.2.5",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-sort-destructure-keys": "2.0.0",
        "fast-check": "4.0.1",
        "glob": "11.0.1",
        "jscodeshift": "17.3.0",
        "madge": "8.0.0",
        "prettier": "3.5.3",
        "prettier-plugin-jsdoc": "1.3.2",
        "prettier-plugin-packagejson": "2.5.10",
        "rimraf": "6.0.1",
        "ssh2": "1.16.0",
        "tsx": "4.19.3",
        "typescript": "5.8.2",
        "typescript-eslint": "8.29.0",
        "undici": "7.6.0",
        "vite": "6.2.4",
        "vitest": "3.1.1",
        "ws": "8.18.1"
    },
    "peerDependencies": {
        "@effect/cluster": "0.29.7",
        "@effect/experimental": "0.44.4",
        "@effect/platform": "0.80.4",
        "@effect/rpc": "0.55.5",
        "@effect/sql": "0.33.4",
        "effect": "3.14.4"
    },
    "optionalDependencies": {
        "@effect/platform-browser": "0.59.4",
        "@effect/platform-bun": "0.60.7",
        "@effect/platform-node": "0.76.7",
        "ssh2": "^1.16.0",
        "undici": "^7.4.0",
        "ws": "^8.18.1"
    },
    "packageManager": "pnpm@9.15.9",
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist",
        "provenance": false
    },
    "pnpm": {
        "patchedDependencies": {
            "@effect/build-utils": "patches/@effect__build-utils.patch"
        }
    }
}
