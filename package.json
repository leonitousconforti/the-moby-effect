        "@rushstack/heft": "0.63.5",
        "@rushstack/heft-jest-plugin": "0.10.7",
        "@rushstack/heft-lint-plugin": "0.2.15",
        "@rushstack/heft-typescript-plugin": "0.2.15",
        "@types/eslint": "^8.56.0",
        "@types/heft-jest": "1.0.6",
        "@types/node": "20.10.5",
        "@types/ssh2": "1.11.18",
        "@types/tar-fs": "2.0.4",
        "effect": "2.0.0-next.62",
        "eslint": "8.56.0",
