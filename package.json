{
    "name": "the-moby-effect",
    "version": "1.45.0-alpha.8",
    "description": "Moby/Docker API client built using effect-ts",
    "keywords": [
        "moby",
        "docker",
        "podman",
        "effect-ts"
    ],
    "homepage": "https://github.com/leonitousconforti/the-moby-effect",
    "bugs": {
        "url": "https://github.com/leonitousconforti/the-moby-effect/issues",
        "email": "leo@leoconforti.us"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leonitousconforti/the-moby-effect.git"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/dts/index.d.ts",
    "directories": {
        "bin": "bin",
        "doc": "docs",
        "example": "examples",
        "test": "test"
    },
    "scripts": {
        "build": "pnpm clean && pnpm codegen && pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v2 && docgen",
        "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
        "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
        "build-esm": "tsc -b tsconfig.build.json",
        "build-reflection": "(cd reflection/ && go build)",
        "build-schemas": "(cd src && rm -rf ./generated && mkdir -p generated && ../reflection/reflection) && eslint ./src/generated/ --fix",
        "check": "tsc -b tsconfig.json",
        "clean": "rimraf .tsbuildinfo docs build dist coverage",
        "codegen": "build-utils prepare-v2",
        "coverage": "vitest --coverage",
        "lint": "eslint .",
        "schemagen": "pnpm build-reflection && pnpm build-schemas",
        "test": "vitest"
    },
    "devDependencies": {
        "@babel/cli": "7.24.8",
        "@babel/core": "7.24.9",
        "@babel/plugin-transform-export-namespace-from": "7.24.7",
        "@babel/plugin-transform-modules-commonjs": "7.24.8",
        "@effect/build-utils": "0.7.7",
        "@effect/cli": "0.39.0",
        "@effect/docgen": "0.4.3",
        "@effect/eslint-plugin": "0.2.0",
        "@effect/experimental": "0.20.1",
        "@effect/language-service": "0.1.0",
        "@effect/platform": "0.60.0",
        "@effect/platform-browser": "0.39.0",
        "@effect/platform-node": "0.55.0",
        "@effect/printer": "0.33.50",
        "@effect/printer-ansi": "0.33.50",
        "@effect/schema": "0.69.0",
        "@effect/typeclass": "0.24.50",
        "@effect/vitest": "0.6.11",
        "@rollup/wasm-node": "4.19.0",
        "@types/node": "20.14.12",
        "@types/ssh2": "1.15.0",
        "@types/tar-fs": "2.0.4",
        "@typescript-eslint/eslint-plugin": "7.17.0",
        "@typescript-eslint/parser": "7.17.0",
        "@typescript-eslint/utils": "7.17.0",
        "@vitest/coverage-v8": "1.6.0",
        "babel-plugin-annotate-pure-calls": "0.4.0",
        "ci-info": "4.0.0",
        "effect": "3.5.7",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "eslint-plugin-codegen": "0.28.0",
        "eslint-plugin-deprecation": "3.0.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-prettier": "5.2.1",
        "eslint-plugin-sort-destructure-keys": "2.0.0",
        "fast-check": "3.20.0",
        "prettier": "3.3.3",
        "prettier-plugin-jsdoc": "1.3.0",
        "prettier-plugin-packagejson": "2.5.1",
        "rimraf": "6.0.1",
        "tar-fs": "3.0.6",
        "tsx": "4.16.2",
        "typescript": "5.5.4",
        "vite": "5.3.4",
        "vitest": "1.6.0",
        "ws": "8.18.0"
    },
    "peerDependencies": {
        "@effect/experimental": "0.20.1",
        "@effect/platform": "0.60.0",
        "@effect/schema": "0.69.0",
        "effect": "3.5.7"
    },
    "optionalDependencies": {
        "@effect/platform-browser": "0.39.0",
        "@effect/platform-node": "0.55.0",
        "ssh2": "^1.15.0",
        "undici": "^6.19.2"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist",
        "provenance": true
    },
    "pnpm": {
        "patchedDependencies": {
            "@effect/build-utils@0.7.7": "patches/@effect__build-utils@0.7.7.patch"
        }
    }
}
