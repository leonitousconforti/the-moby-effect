{
    "name": "the-moby-effect",
    "version": "1.43.1",
    "description": "Docker daemon API client built using effect-ts",
    "keywords": [
        "moby",
        "docker"
    ],
    "homepage": "https://github.com/leonitousconforti/the-moby-effect",
