{
    "name": "the-moby-effect",
    "version": "1.48.3",
    "description": "Moby/Docker API client built using effect-ts",
    "keywords": [
        "moby",
        "docker",
        "podman",
        "effect-ts"
    ],
    "homepage": "https://github.com/leonitousconforti/the-moby-effect",
    "bugs": {
        "url": "https://github.com/leonitousconforti/the-moby-effect/issues",
        "email": "leo@leoconforti.us"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leonitousconforti/the-moby-effect.git"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": "./src/index.ts",
        "./*": "./src/*.ts",
        "./internal/*": null
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/dts/index.d.ts",
    "directories": {
        "bin": "bin",
        "doc": "docs",
        "example": "examples",
        "test": "test"
    },
    "scripts": {
        "build": "pnpm clean && pnpm codegen && pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v3",
        "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
        "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
        "build-esm": "tsc -b tsconfig.build.json",
        "build-reflection": "(cd reflection/ && go build)",
        "build-schemas": "(cd src && rm -rf ./generated && mkdir -p generated && ../reflection/reflection) && eslint ./src/generated/ --fix",
        "check": "tsc -b tsconfig.json",
        "circular": "node ./scripts/circular.mjs",
        "clean": "rimraf .tsbuildinfo docs build dist coverage",
        "codegen": "build-utils prepare-v3",
        "codemod": "node scripts/codemod.mjs",
        "coverage": "vitest --coverage",
        "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
        "schemagen": "pnpm build-reflection && pnpm build-schemas",
        "test": "vitest",
        "update-blobs": "tsx ./scripts/update-blobs.ts"
    },
    "dependencies": {
        "eftar": "0.0.56"
    },
    "devDependencies": {
        "@babel/cli": "7.27.0",
        "@babel/core": "7.26.10",
        "@babel/plugin-transform-export-namespace-from": "7.25.9",
        "@babel/plugin-transform-modules-commonjs": "7.26.3",
        "@changesets/cli": "2.29.2",
        "@effect/build-utils": "0.8.3",
        "@effect/cli": "0.59.14",
        "@effect/cluster": "0.30.2",
        "@effect/docgen": "https://pkg.pr.new/Effect-TS/docgen/@effect/docgen@57ff897",
        "@effect/experimental": "0.44.14",
        "@effect/language-service": "0.8.1",
        "@effect/platform": "0.80.14",
        "@effect/platform-browser": "0.60.5",
        "@effect/platform-bun": "0.61.2",
        "@effect/platform-node": "0.77.2",
        "@effect/printer": "0.42.14",
        "@effect/printer-ansi": "0.42.14",
        "@effect/rpc": "0.56.2",
        "@effect/sql": "0.33.14",
        "@effect/typeclass": "0.33.14",
        "@effect/vitest": "0.20.15",
        "@eslint/js": "9.25.1",
        "@types/node": "22.15.3",
        "@types/ssh2": "1.15.5",
        "@types/ws": "8.18.1",
        "@typescript-eslint/parser": "8.31.1",
        "@vitest/coverage-v8": "3.1.2",
        "ast-types": "0.14.2",
        "babel-plugin-annotate-pure-calls": "0.5.0",
        "changesets-gitlab": "0.13.3",
        "effect": "3.14.14",
        "eslint": "9.25.1",
        "eslint-config-prettier": "10.1.2",
        "eslint-import-resolver-typescript": "4.3.4",
        "eslint-plugin-import-x": "4.11.0",
        "eslint-plugin-prettier": "5.2.6",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-sort-destructure-keys": "2.0.0",
        "fast-check": "4.1.1",
        "glob": "11.0.2",
        "jscodeshift": "17.3.0",
        "madge": "8.0.0",
        "prettier": "3.5.3",
        "prettier-plugin-jsdoc": "1.3.2",
        "prettier-plugin-packagejson": "2.5.10",
        "rimraf": "6.0.1",
        "ssh2": "1.16.0",
        "tsx": "4.19.3",
        "typescript": "5.8.3",
        "typescript-eslint": "8.31.1",
        "undici": "7.8.0",
        "unrs-resolver": "1.7.2",
        "vite": "6.3.3",
        "vitest": "3.1.2",
        "ws": "8.18.1"
    },
    "peerDependencies": {
        "@effect/cluster": "0.30.2",
        "@effect/experimental": "0.44.14",
        "@effect/platform": "0.80.14",
        "@effect/platform-browser": "0.60.5",
        "@effect/platform-bun": "0.61.2",
        "@effect/platform-node": "0.77.2",
        "@effect/rpc": "0.56.2",
        "@effect/sql": "0.33.14",
        "effect": "3.14.14"
    },
    "peerDependenciesMeta": {
        "@effect/platform-browser": {
            "optional": true
        },
        "@effect/platform-bun": {
            "optional": true
        },
        "@effect/platform-node": {
            "optional": true
        }
    },
    "optionalDependencies": {
        "ssh2": "^1.16.0",
        "undici": "^7.7.0",
        "ws": "^8.18.1"
    },
    "packageManager": "pnpm@9.15.9",
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist",
        "linkDirectory": false,
        "provenance": false
    },
    "pnpm": {
        "patchedDependencies": {
            "@effect/build-utils": "patches/@effect__build-utils.patch"
        }
    }
}
